# Example environment file for SCIM API configuration
# Copy this file to .env and fill in your actual values

# Cato Networks SCIM API Base URL
# Format: https://your-account.catonetworks.com/api/v1/scim/v2
SCIM_BASE_URL=https://your-account.catonetworks.com/api/v1/scim/v2

# Cato Networks SCIM Bearer Token
# Obtain from Cato Management Application under Administration > API
SCIM_TOKEN=your-bearer-token-here

# Enable debug logging (true/false)
DEBUG=false